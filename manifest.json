{
    "manifest_version": 3,
    "name": "Show Cookies",
    "version": "1.0",
    "description": "Display all cookies for the current tab.",
    "permissions": [
      "cookies",
      "activeTab",
      "scripting",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Show Cookies"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  